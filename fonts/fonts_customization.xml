<fonts-modification version="1">
<!-- Cool Jazz -->
    <family customizationType="new-named-family" name="cooljazz">
        <font weight="300" style="normal">Cooljazz.ttf</font>
    </family>
    <alias name="cooljazz-light" to="cooljazz" weight="300" />
<!-- Coolstory -->
    <family customizationType="new-named-family" name="coolstory">
        <font weight="300" style="normal">Coolstory-Regular.ttf</font>
    </family>
    <alias name="coolstory-light" to="coolstory" weight="300" />
 <!-- EvolveSans -->   
<family customizationType="new-named-family" name="evolvesans">
        <font weight="400" style="normal">EvolveSans.ttf</font>
    </family>
    <alias name="evolvesans-semi-bold" to="evolvesans" weight="600" />
    <alias name="evolvesans-bold" to="evolvesans" weight="700" />
    <alias name="times" to="evolvesans" />
    <alias name="times new roman" to="evolvesans" />
    <alias name="palatino" to="evolvesans" />
    <alias name="georgia" to="evolvesans" />
    <alias name="baskerville" to="evolvesans" />
    <alias name="goudy" to="evolvesans" />
    <alias name="fantasy" to="evolvesans" />
    <alias name="ITC Stone Serif" to="evolvesans" />
 <!-- Exotwo -->
    <family customizationType="new-named-family" name="exotwo">
        <font weight="400" style="normal">Exotwo.ttf</font>
    </family>
    <alias name="exotwo-regular" to="exotwo" weight="400" />
  <!-- fucek -->
 <family customizationType="new-named-family" name="fucek">
        <font weight="400" style="normal">Fucek.ttf</font>
    </family>
    <alias name="fucek-semi-bold" to="fucek" weight="600" />
    <alias name="fucek-bold" to="fucek" weight="700" />
    <alias name="times" to="fucek" />
    <alias name="times new roman" to="fucek" />
    <alias name="palatino" to="fucek" />
    <alias name="georgia" to="fucek" />
    <alias name="baskerville" to="fucek" />
    <alias name="goudy" to="fucek" />
    <alias name="fantasy" to="fucek" />
    <alias name="ITC Stone Serif" to="fucek" />
</fonts-modification>
